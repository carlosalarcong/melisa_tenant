{
    "controllers": {
        "@symfony/ux-turbo": {
            "turbo-core": {
                "enabled": true,
                "fetch": "eager"
            },
            "mercure-turbo-stream": {
                "enabled": false,
                "fetch": "eager"
            }
        },
        "mantenedores--menu": {
            "enabled": true,
            "fetch": "lazy"
        },
        "mantenedores--modal": {
            "enabled": true,
            "fetch": "lazy"
        },
        "mantenedores--list": {
            "enabled": true,
            "fetch": "lazy"
        },
        "mantenedores--notification": {
            "enabled": true,
            "fetch": "lazy"
        }
    },
    "entrypoints": [],
    "comments": {
        "dynamic_loading": "Controllers are now loaded dynamically using DynamicControllerLoader",
        "structure": {
            "internal": "Controllers for internal use (forms, UI interactions)",
            "apiplatform": "Controllers for API Platform integration",
            "subdomain_fallback": "Controllers load from subdomain folder, fallback to default",
            "mantenedores": "Controllers for maintainer system with dynamic tenant functionality"
        },
        "usage_examples": {
            "internal_patient": "data-controller='internal--patient'",
            "api_patient": "data-controller='apiplatform--api-patient'",
            "mantenedores_menu": "data-controller='mantenedores--menu'",
            "mantenedores_modal": "data-controller='mantenedores--modal'"
        }
    }
}
